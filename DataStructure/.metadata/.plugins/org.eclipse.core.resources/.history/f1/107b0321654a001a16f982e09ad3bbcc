
public class ROtation {
public static void main(String[] args) {
	
	
	int a[]= {1,2,3,4,5,6,7,8,9};

	int numberOfRotataions = 3;
	
	int temp;
	
	
	while()
	{
		for (int i = 0; i < a.length; i++) {
			
			if (numberOfRotataions-- > 0) {
				temp = a[i];
			}
			
			
			a[i]=a[i+1];
		
	}
	
	}
	
}
}
