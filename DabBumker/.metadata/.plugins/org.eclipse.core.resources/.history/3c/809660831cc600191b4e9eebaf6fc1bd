
public class BasicStack <X> {

	private X[] data;
	private int stackpointer;
	
	
	public BasicStack()
	{
		stackpointer=0;
		data = (X[]) new Object[1000];
	}
	
	
	public void push(X item)
	{
		data[stackpointer++] = item;
	}
	
	public X pop()
	{
		
	}
	
	public boolean contains()
	{
		
	}
	
	public X access()
	{
		
	}
	
	public int size()
	{
		return stackpointer;
	}
}
